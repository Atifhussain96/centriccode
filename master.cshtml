@using Umbraco.Core.PropertyEditors
@using ClientDependency.Core.Mvc
@inherits UmbracoViewPage
@{
    Layout = null;

    var lang = System.Globalization.CultureInfo.CurrentCulture.TwoLetterISOLanguageName;
    var currLang = System.Globalization.CultureInfo.CurrentCulture.TwoLetterISOLanguageName;

    // Get basic design settings from the Master
    var master = (Master)Model.Root();

    Html.RequiresCss("~/assets/css/bootstrap.min.css");
    Html.RequiresCss("~/assets/css/fontawesome.min.css");
    Html.RequiresCss("~/css/slick.css");
    Html.RequiresCss("~/css/aos.css");
    Html.RequiresCss("~/css/main.min.css");


    Html.RequiresJs("~/scripts/jquery-3.5.1.min.js");
    Html.RequiresJs("~/scripts/jquery.validate.min.js");
    Html.RequiresJs("~/scripts/jquery.validate.unobtrusive.min.js");
    Html.RequiresJs("~/scripts/popper.min.js");
    Html.RequiresJs("~/scripts/slick.min.js");
    Html.RequiresJs("~/scripts/aos.js");
    Html.RequiresJs("~/scripts/custom.js");
}
@functions {
    public class categoryClass
    {
        public int categoryId { get; set; }
        public int no { get; set; }
    }

    private string GetMeta(IPublishedContent content, string viewBagValue, string seoFieldName)
    {
        // by default use seo field value, fall back to viewbag value if supplied
        return content.HasValue(seoFieldName) ? content.Value<string>(seoFieldName) : viewBagValue;
    }
}
